{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*", "/css/*", "/js/*", "/assets/*"]
  },
  "globalHeaders": {
    "Content-Security-Policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval' *.supabase.co unpkg.com *.openstreetmap.org *.tile.openstreetmap.org data: blob: https:;",
    "X-Frame-Options": "DENY",
    "X-Content-Type-Options": "nosniff"
  },
  "mimeTypes": {
    ".json": "text/json"
  }
}